<h1 class="text-center"> WELL...ho kay </h1>

<% @posts.each do |post| %>
	<div class="launch-box span3 text-center">
		<h1>
			<%= post.title %>
		</h1>


		<% if post.image.present? %>
		<%= image_tag post.image %>
		<% end %>

			<h2>

		<%= post.description %>

			</h2>
	</div>
	
	
<% end %>